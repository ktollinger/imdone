{
  "name": "imdone",
  "version": "1.3.1",
  "homepage": "http://piascikj.github.io/imdone/",
  "authors": [
    "Jesse Piascik <jesse@piascik.net>"
  ],
  "description": "A simple scrum board and markdown wiki.",
  "main": "lib/imdone.js",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.0",
    "bootswatch": "*",
    "backbone": "*",
    "bootstrap": "~3.1.1",
    "marked": "~0.3.1",
    "prismjs": "*",
    "underscore": "*",
    "requirejs": "*",
    "jquery.scrollTo": "~1.4.6",
    "font-awesome": "4.x.x",
    "json2": "*",
    "handlebars": "*",
    "jquery.hotkeys": "*",
    "store": "~2.1.3",
    "ace-builds": "~1.1.3",
    "zeroclipboard": "~1.3.0-beta.1",
    "printThis": "git://github.com/imdone/printThis#master",
    "jquery.toc": "~0.3.5",
    "intro.js": "git://github.com/piascikj/intro.js#master"
  },
  "exportsOverride": {
    "requirejs": {
      "": "require.js"
    },
    "jquery": {
      "": [
        "dist/*.js",
        "dist/*.map"
      ]
    },
    "underscore": {
      "": "underscore.js"
    },
    "marked": {
      "": "lib/marked.js"
    },
    "backbone": {
      "": "backbone*"
    },
    "bootstrap": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css",
      "fonts": "dist/fonts/*"
    },
    "bootswatch": {
      "css": "slate/*.css",
      "fonts": "../bootstrap/dist/fonts/*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "font": "font/*"
    },
    "prismjs": {
      "css": "*.css",
      "js": "*.js"
    },
    "handlebars": {
      "": [
        "handlebars.js",
        "handlebars.min.js"
      ]
    },
    "jquery.hotkeys": {
      "": "jquery.hotkeys.js"
    },
    "ace-builds": {
      "snippets": "src-noconflict/snippets/*.js",
      "": "src-noconflict/*.js"
    },
    "zeroclipboard": {
      "js": "ZeroClipboard*.js",
      "swf": "*.swf"
    },
    "printThis": {
      "": "printThis.js"
    },
    "intro.js": {
      "js": "intro.js",
      "css": "introjs.css"
    }
  },
  "resolutions": {
    "bootstrap": "~3.1.1"
  }
}
