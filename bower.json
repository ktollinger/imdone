{
  "name": "imdone",
  "version": "0.4.9",
  "homepage": "http://piascikj.github.io/imdone/",
  "authors": [
    "Jesse Piascik <jesse@piascik.net>"
  ],
  "description": "A simple scrum board and markdown wiki.",
  "main": "lib/imdone.js",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~1.8.3",
    "bootswatch": "*",
    "backbone": "*",
    "bootstrap": "~3.0.0",
    "jquery.toc": "~0.3.5",
    "marked": "~0.3.1",
    "prismjs": "*",
    "underscore": "*",
    "requirejs": "*",
    "jquery.scrollTo": "~1.4.6",
    "font-awesome": "~3.2.0",
    "json2": "*",
    "handlebars": "~1.1.2",
    "jquery.hotkeys": "*",
    "store": "~2.1.3",
    "ace-builds": "~1.1.1",
    "zeroclipboard": "~1.3.0-beta.1"
  },
  "exportsOverride": {
    "requirejs": {
      "": "require.js"
    },
    "jquery": {
      "": [
        "*.js",
        "*.map"
      ]
    },
    "underscore": {
      "": [
        "underscore-min.js",
        "underscore-min.map"
      ]
    },
    "marked": {
      "": "lib/marked.js"
    },
    "backbone": {
      "": "backbone*"
    },
    "bootstrap": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css",
      "fonts": "dist/fonts/*"
    },
    "bootswatch": {
      "css": "cerulean/*.css",
      "fonts": "../bootstrap/dist/fonts/*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "font": "font/*"
    },
    "prismjs": {
      "css": "*.css",
      "js": "*.js"
    },
    "handlebars": {
      "": [
        "handlebars.js",
        "handlebars.min.js"
      ]
    },
    "jquery.hotkeys": {
      "": "jquery.hotkeys.js"
    },
    "ace-builds": {
      "":"src-min/*.js"
    },
    "zeroclipboard": {
      "js": "ZeroClipboard*.js",
      "swf": "*.swf"
    }
  }
}
